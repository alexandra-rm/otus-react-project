{
  "name": "otus-react-project",
  "version": "1.0.0",
  "description": "OTUS React JS Courses Final Project",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "start:prod": "webpack-dev-server --mode production --open",
    "build": "webpack",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "prettify": "npx prettier . --write",
    "test": "jest",
    "coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "loki:update": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve",
    "test:loki": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static"
  },
  "author": "Evgeny Shurupov",
  "license": "ISC",
  "homepage": "https://github.com/shurupov/otus-react-project",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-essentials": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/source-loader": "^5.3.18",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.45",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "react-is": "^16.13.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "loki": "^0.20.3"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.34",
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
