{
    "name": "otus-react-project",
    "version": "1.0.0",
    "description": "OTUS React JS Courses Final Project",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server --mode development --open --hot",
        "start:prod": "npx webpack-dev-server --mode production --open",
        "build": "npx webpack",
        "start:heroku": "npm install --save webpack webpack-dev-server && npm run build && npm run start:prod",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix ./",
        "prettify": "npx prettier . --write",
        "test": "jest",
        "coverage": "jest --coverage",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "loki:update": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
        "loki:approve": "npx loki approve",
        "test:loki": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static",
        "check": "npm test && npm run test:loki",
        "deploy": "gh-pages -d dist"
    },
    "author": "Evgeny Shurupov",
    "license": "ISC",
    "homepage": "https://shurupov.github.io/otus-react-project",
    "devDependencies": {
        "@babel/cli": "^7.10.5",
        "@babel/core": "^7.11.1",
        "@babel/polyfill": "^7.10.4",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "@babel/preset-typescript": "^7.10.4",
        "@storybook/addon-actions": "^5.3.18",
        "@storybook/addon-essentials": "^5.3.18",
        "@storybook/addon-knobs": "^5.3.18",
        "@storybook/addon-links": "^5.3.18",
        "@storybook/addon-storysource": "^5.3.18",
        "@storybook/react": "^5.3.18",
        "@storybook/source-loader": "^5.3.18",
        "@types/enzyme": "^3.10.5",
        "@types/jest": "^26.0.9",
        "@types/lodash": "^4.14.159",
        "@types/react": "^16.9.45",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/react-router-dom": "^5.1.5",
        "@typescript-eslint/eslint-plugin": "^3.9.1",
        "@typescript-eslint/parser": "^3.9.1",
        "babel-jest": "^26.2.2",
        "babel-loader": "^8.1.0",
        "babel-plugin-emotion": "^10.0.33",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.3",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "gh-pages": "^3.1.0",
        "html-webpack-plugin": "^4.3.0",
        "jest": "^26.2.2",
        "loki": "^0.20.3",
        "prettier": "^2.0.5",
        "react-is": "^16.13.1",
        "redux-saga-test-plan": "^4.0.0-rc.3",
        "typescript": "^3.9.7",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "@emotion/babel-preset-css-prop": "^10.0.27",
        "@emotion/core": "^10.0.34",
        "@material-ui/core": "^4.11.0",
        "@reduxjs/toolkit": "^1.4.0",
        "fontsource-roboto": "^3.0.3",
        "lodash": "^4.17.19",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.1",
        "react-router-dom": "^5.2.0",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-saga": "^1.1.3"
    }
}
